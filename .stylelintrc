{
  "extends": [
    "stylelint-config-standard",
    "stylelint-config-recess-order"
  ],
  "syntax": "scss",
  "plugins": [
    "stylelint-order",
    "stylelint-scss"
  ],
  "rules": {
    "number-leading-zero": "never",
    "at-rule-no-unknown": null,
    "scss/at-rule-no-unknown": true,
    "indentation": 2
  }
}
